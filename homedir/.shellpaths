#########################################################################
# My Scripts
#########################################################################
export PATH=$PATH:.

#########################################################################
# Tools
#########################################################################
export PATH=/usr/local/sbin:$PATH
export PATH=/usr/local/bin:$PATH
export PATH=$PATH:/usr/local/opt/go/libexec/bin

export PATH=$PATH:$HOME/.pyenv
export PATH=$PATH:$GOPATH/bin

